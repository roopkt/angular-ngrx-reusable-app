{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noStrictGenericChecks": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "../out-tsc/spec",
    "target": "es5",
    "module": "commonjs",
    "baseUrl": "",
    "rootDir": "./",
    "strict": true,
    "paths": {
      "@branding/rootstate": [
        "../rootstate/src"
      ]
    },
    "strictNullChecks": false
  },
  "exclude": [
    "node_modules",
    "**/*/node_modules"
  ],
  "include": [
   "../product-management/**/*"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}