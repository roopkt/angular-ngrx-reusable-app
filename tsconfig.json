{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noStrictGenericChecks": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "outDir": "../out-tsc/app",
    "target": "es5",
    "module": "commonjs",
    "baseUrl": "",
    "rootDir": "./",
    "strict": true,
    "paths": {
      "@branding/rootstate": [
        "./lib/rootstate"
      ],
      "@wonderful/project-management": [
        "./lib/product-management"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
  },
  "exclude": [
    "node_modules",
    "**/*/node_modules",
    "modules/schematics/src/*/files/**/*"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}